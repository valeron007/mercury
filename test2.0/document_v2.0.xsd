      <xs:annotation>
         <xs:documentation>Тип, описывающий список ролей пользователя.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ID" type="xs:NCName" minOccurs="1" maxOccurs="1"/>
         <xs:element name="name" type="bs:String255" minOccurs="0" maxOccurs="1"/>
         <xs:element name="granted" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
   </xs:complexType>

