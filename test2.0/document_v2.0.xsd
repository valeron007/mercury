   <xs:complexType name="VetDocumentStatusChange">
      <xs:annotation>
         <xs:documentation>Сведения об изменении статуса ветеринарного документа</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="status" type="vd:VetDocumentStatus" minOccurs="1" maxOccurs="1">
