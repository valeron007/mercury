      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:EntityList">
            <xs:sequence>
               <xs:element name="workingArea" type="vd:WorkingArea" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

