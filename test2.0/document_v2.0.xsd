                     <xs:documentation>Имя пользователя в системе Ветис.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="firstName" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Имя пользователя.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="middleName" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Отчество пользователя.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="lastName" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Фамилия пользователя.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="fio" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>ФИО пользователя.</xs:documentation>
                  </xs:annotation>
               </xs:element>
