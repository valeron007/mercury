               <xs:documentation>Установленный статус документа.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="specifiedPerson" type="vd:User" minOccurs="1" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Пользователь, изменивший статус ВСД (например, подписавший или аннулировавший его).
               </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="actualDateTime" type="xs:dateTime" minOccurs="1" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Дата и время изменения статуса ВСД.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="reason" type="vd:VetDocumentStatusChangeReason" minOccurs="0" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Основание изменения статуса ВСД (например, причина аннулирования сертификата).
               </xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>

   <xs:simpleType name="VetDocumentStatusChangeReason">
      <xs:annotation>
         <xs:documentation>Основание изменения статуса ВСД</xs:documentation>
      </xs:annotation>
      <xs:restriction base="bs:Text"/>
   </xs:simpleType>

   <xs:complexType name="RegionalizationClause">
