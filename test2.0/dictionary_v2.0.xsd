   <xs:simpleType name="ResearchResult">
      <xs:annotation>
         <xs:documentation>Результат лабораторного исследования/ВСЭ</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="UNKNOWN">
            <xs:annotation>
               <xs:documentation>Результат неизвестен</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UNDEFINED">
            <xs:annotation>
               <xs:documentation>Результат невозможно определить (не нормируется)</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="POSITIVE">
            <xs:annotation>
               <xs:documentation>Положительный результат</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="NEGATIVE">
            <xs:annotation>
               <xs:documentation>Отрицательный результат</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UNFULFILLED">
            <xs:annotation>
               <xs:documentation>Не проводилось</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VSERAW">
            <xs:annotation>
               <xs:documentation>ВСЭ подвергнуто сырьё, из которого произведена продукция</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="VSEFULL">
            <xs:annotation>
               <xs:documentation>Продукция подвергнута ВСЭ в полном объеме</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>

   <xs:complexType name="Indicator">
      <xs:annotation>
         <xs:documentation>Показатель безопасности</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="name" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Наименование показателя</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="AnimalDisease">
      <xs:annotation>
         <xs:documentation>Заболевание</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="name" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Наименование заболевания</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="ResearchMethod">
      <xs:annotation>
         <xs:documentation>Метод исследования</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="name" type="bs:String255" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Наименование метода исследования</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="MedicinalDrug">
      <xs:annotation>
         <xs:documentation>Препарат/вакцина</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ID" type="bs:Identifier" minOccurs="0" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Идентификационный (уникальный) номер препарата</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="name" type="bs:String255" minOccurs="0" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Наименование препарата</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="series" type="bs:String255" minOccurs="0" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Номер серии препарата/вакцины</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="producer" type="dt:BusinessMember" minOccurs="0" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Производитель препарата</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>

   <xs:simpleType name="OTPToken">
      <xs:annotation>
         <xs:documentation>Одноразовый токен</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:token"/>
   </xs:simpleType>

   <xs:complexType name="RegionalizationCondition">
      <xs:annotation>
         <xs:documentation>Условие регионализации, соблюдаемое при перевозке груза</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="referenceNumber" type="bs:Identifier" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Номер условия</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="text" type="bs:Text" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Формулировка условия</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="strict" type="xs:boolean" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Обязательность соблюдения условия</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="relatedDisease" type="dt:AnimalDisease" minOccurs="0" maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Заболевание, к которому относится данное условие</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="RegionalizationConditionGroup">
      <xs:annotation>
         <xs:documentation>Группа условий регионализации, соблюдаемых при перевозке груза.
            Считается, что группа условий соблюдается (выполняется), если соблюдается каждое условие, входящее в группу.
         </xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="condition" type="dt:RegionalizationCondition" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="RegionalizationRequirement">
      <xs:annotation>
         <xs:documentation>Требование к перемещению партии в отношении к определенному заболеванию.
         </xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="relatedDisease" type="dt:AnimalDisease" minOccurs="0" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Заболевание, к которому относится данное требование</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="type" type="dt:RegionalizationDecision" minOccurs="1" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Вид требования: перемещение запрещено, перемещение разрешено
                  или перемещение разрешено при обязательном соблюдении условий.
               </xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="conditionGroup" type="dt:RegionalizationConditionGroup" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>Группа условий регионализации, соблюдаемых при перевозке продукции.
                  Имеет смысл, если тип требования `type` имеет значение 3 (перемещение разрешено при обязательном соблюдении условий).
                  В этом случае требование считается выполненным, если выполнена одна из альтернативных групп условий `conditionGroup`.
                  См. условие выполнения группы в описании типа `dt:RegionalizationConditionGroup`.
               </xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="RegionalizationShippingRule">
      <xs:annotation>
         <xs:documentation>Правило перемещения партии согласно регионализации</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="referenceNumber" type="bs:Identifier" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Номер правила перевозки в реестре.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="fromR13nStatus" type="dt:RegionalizationStatus" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Статус территории, откуда партия отправляется (место отправления), согласно регионализации.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="toR13nStatus" type="dt:RegionalizationStatus" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Статус территории, куда партия направляется (место назначения), согласно регионализации.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="cargoType" type="dt:SubProduct" minOccurs="0" maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Ветеринарная категория груза, на которую распространяется правило перемещения.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="decision" type="dt:RegionalizationDecision" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Решение о возможности перемещения груза:
                        перемещение запрещено, перемещение разрешено
                        или перемещение разрешено при обязательном соблюдении условий.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="requirement" type="dt:RegionalizationRequirement" minOccurs="0" maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Требование к перемещению партии в отношении к определенному заболеванию.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:simpleType name="RegionalizationDecision">
      <xs:annotation>
         <xs:documentation>Вид требования: перемещение запрещено, перемещение разрешено
            или перемещение разрешено при обязательном соблюдении условий.
         </xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:token">
         <xs:enumeration value="1">
            <xs:annotation>
               <xs:documentation>Перемещение разрешено</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="2">
            <xs:annotation>
               <xs:documentation>Перемещение разрешено при обязательном соблюдении условий</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="3">
            <xs:annotation>
               <xs:documentation>Перемещение запрещено</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="ProsperityType">
      <xs:annotation>
         <xs:documentation>Статус благополучия</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:token">
         <xs:enumeration value="UNDEFINED">
            <xs:annotation>
               <xs:documentation>Неопределенный (не определялся)</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UNKNOWN">
            <xs:annotation>
               <xs:documentation>Неидентифицированный/неизвестный (статус невозможно определить)</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="SAFE">
            <xs:annotation>
               <xs:documentation>Благополучный</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UNSAFE">
            <xs:annotation>
               <xs:documentation>Неблагополучный</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>

   <xs:simpleType name="VaccinationType">
      <xs:annotation>
         <xs:documentation>Статус применения вакцины</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:token">
         <xs:enumeration value="UNDEFINED">
            <xs:annotation>
               <xs:documentation>Неопределенный</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="APPLIED">
            <xs:annotation>
               <xs:documentation>Вакцинация проводится</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="UNVACCINATED">
            <xs:annotation>
               <xs:documentation>Вакцинация не проводится</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>

   <xs:complexType name="RegionalizationStatus">
      <xs:annotation>
         <xs:documentation>Статус региона в соответствии с регионализацией</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="relatedDisease" type="dt:AnimalDisease" minOccurs="1" maxOccurs="1">
            <xs:annotation>
               <xs:documentation>Заболевание, по которому назначен статус</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="prosperity" type="dt:ProsperityType" minOccurs="1" maxOccurs="1"/>
         <xs:element name="vaccination" type="dt:VaccinationType" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
   </xs:complexType>

   <xs:complexType name="RegionalizationRegionStatus">
      <xs:annotation>
         <xs:documentation>Статус регионализации</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:GenericVersioningEntity">
            <xs:sequence>
               <xs:element name="referenceNumber" type="bs:Identifier" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Номер статуса региона в реестре.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="r13nZone" type="dt:Area" minOccurs="0" maxOccurs="1">
                  <xs:annotation>
                     <xs:documentation>Территория/регион</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="excludedR13nZone" type="dt:Area" minOccurs="0" maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Территория/регион внутри `r13Zone` с отличным (другим) статусом регионализации.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="r13nStatus" type="dt:RegionalizationStatus" minOccurs="0" maxOccurs="unbounded">
                  <xs:annotation>
                     <xs:documentation>Статус регионализации</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="Area">
      <xs:annotation>
         <xs:documentation>Территория: регион, район, город или область, ограниченная полигоном.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="dt:Address"/>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="RegionalizationConditionList">
      <xs:annotation>
         <xs:documentation>Тип, описывающий список условий перемещения.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:EntityList">
            <xs:sequence>
               <xs:element name="condition" type="dt:RegionalizationCondition" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="RegionalizationRegionStatusList">
      <xs:annotation>
         <xs:documentation>Тип, описывающий список статусов региона (регионализация).</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:EntityList">
            <xs:sequence>
               <xs:element name="status" type="dt:RegionalizationRegionStatus" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="RegionalizationShippingRuleList">
      <xs:annotation>
         <xs:documentation>Тип, описывающий список правил перемещения (регионализация).</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:EntityList">
            <xs:sequence>
               <xs:element name="rule" type="dt:RegionalizationShippingRule" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="AnimalDiseaseList">
      <xs:annotation>
         <xs:documentation>Тип, описывающий список заболеваний.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:EntityList">
            <xs:sequence>
               <xs:element name="disease" type="dt:AnimalDisease" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="ResearchMethodList">
      <xs:annotation>
         <xs:documentation>Тип, описывающий список методов исследований.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:EntityList">
            <xs:sequence>
               <xs:element name="method" type="dt:ResearchMethod" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:complexType name="ActivityLocationList">
      <xs:annotation>
         <xs:documentation>Тип, описывающий список методов исследований.</xs:documentation>
      </xs:annotation>
      <xs:complexContent>
         <xs:extension base="bs:EntityList">
            <xs:sequence>
               <xs:element name="location" type="dt:BusinessMember" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
</xs:schema>