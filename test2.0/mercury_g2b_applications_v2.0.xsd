<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://api.vetrf.ru/schema/cdm/mercury/g2b/applications/v2"
           xmlns:merc="http://api.vetrf.ru/schema/cdm/mercury/g2b/applications/v2"
