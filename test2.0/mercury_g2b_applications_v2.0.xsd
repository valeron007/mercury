           xmlns:bs="http://api.vetrf.ru/schema/cdm/base"
           xmlns:apl="http://api.vetrf.ru/schema/cdm/application"
           xmlns:dt="http://api.vetrf.ru/schema/cdm/dictionary/v2"
           xmlns:vd="http://api.vetrf.ru/schema/cdm/mercury/vet-document/v2"
           elementFormDefault="qualified" version="2.0">

   <xs:import namespace="http://api.vetrf.ru/schema/cdm/base" schemaLocation="base_v1.1.xsd"/>
   <xs:import namespace="http://api.vetrf.ru/schema/cdm/application" schemaLocation="application_v1.1.xsd"/>
   <xs:import namespace="http://api.vetrf.ru/schema/cdm/dictionary/v2" schemaLocation="dictionary_v2.0.xsd"/>
   <xs:import namespace="http://api.vetrf.ru/schema/cdm/mercury/vet-document/v2" schemaLocation="document_v2.0.xsd"/>

   <xs:element name="processIncomingConsignmentRequest" type="merc:ProcessIncomingConsignmentRequest"/>
   <xs:element name="processIncomingConsignmentResponse" type="merc:ProcessIncomingConsignmentResponse"/>

   <xs:element name="prepareOutgoingConsignmentRequest" type="merc:PrepareOutgoingConsignmentRequest"/>
   <xs:element name="prepareOutgoingConsignmentResponse" type="merc:PrepareOutgoingConsignmentResponse"/>

   <xs:element name="registerProductionOperationRequest" type="merc:RegisterProductionOperationRequest"/>
   <xs:element name="registerProductionOperationResponse" type="merc:RegisterProductionOperationResponse"/>

   <xs:element name="mergeStockEntriesRequest" type="merc:MergeStockEntriesRequest"/>
   <xs:element name="mergeStockEntriesResponse" type="merc:MergeStockEntriesResponse"/>

   <xs:element name="withdrawVetDocumentRequest" type="merc:WithdrawVetDocumentRequest"/>
   <xs:element name="withdrawVetDocumentResponse" type="merc:WithdrawVetDocumentResponse"/>

   <xs:element name="modifyBusinessEntityRequest" type="merc:ModifyBusinessEntityRequest"/>
   <xs:element name="modifyBusinessEntityResponse" type="merc:ModifyBusinessEntityResponse"/>

   <xs:element name="modifyEnterpriseRequest" type="merc:ModifyEnterpriseRequest"/>
   <xs:element name="modifyEnterpriseResponse" type="merc:ModifyEnterpriseResponse"/>

   <xs:element name="modifyActivityLocationsRequest" type="merc:ModifyActivityLocationsRequest"/>
   <xs:element name="modifyActivityLocationsResponse" type="merc:ModifyActivityLocationsResponse"/>

   <xs:element name="resolveDiscrepancyRequest" type="merc:ResolveDiscrepancyRequest"/>
   <xs:element name="resolveDiscrepancyResponse" type="merc:ResolveDiscrepancyResponse"/>

   <xs:element name="modifyProducerStockListRequest" type="merc:ModifyProducerStockListRequest"/>
   <xs:element name="modifyProducerStockListResponse" type="merc:ModifyProducerStockListResponse"/>

   <xs:element name="getVetDocumentByUuidRequest" type="merc:GetVetDocumentByUuidRequest"/>
   <xs:element name="getVetDocumentByUuidResponse" type="merc:GetVetDocumentByUuidResponse"/>

   <xs:element name="getVetDocumentListRequest" type="merc:GetVetDocumentListRequest"/>
   <xs:element name="getVetDocumentListResponse" type="merc:GetVetDocumentListResponse"/>

   <xs:element name="getVetDocumentChangesListRequest" type="merc:GetVetDocumentChangesListRequest"/>
   <xs:element name="getVetDocumentChangesListResponse" type="merc:GetVetDocumentChangesListResponse"/>

   <xs:element name="getStockEntryByGuidRequest" type="merc:GetStockEntryByGuidRequest"/>
   <xs:element name="getStockEntryByGuidResponse" type="merc:GetStockEntryByGuidResponse"/>

   <xs:element name="getStockEntryByUuidRequest" type="merc:GetStockEntryByUuidRequest"/>
   <xs:element name="getStockEntryByUuidResponse" type="merc:GetStockEntryByUuidResponse"/>

   <xs:element name="getStockEntryListRequest" type="merc:GetStockEntryListRequest"/>
   <xs:element name="getStockEntryListResponse" type="merc:GetStockEntryListResponse"/>

   <xs:element name="getStockEntryChangesListRequest" type="merc:GetStockEntryChangesListRequest"/>
   <xs:element name="getStockEntryChangesListResponse" type="merc:GetStockEntryChangesListResponse"/>

   <xs:element name="getStockEntryVersionListRequest" type="merc:GetStockEntryVersionListRequest"/>
   <xs:element name="getStockEntryVersionListResponse" type="merc:GetStockEntryVersionListResponse"/>

   <xs:element name="updateTransportMovementDetailsRequest" type="merc:UpdateTransportMovementDetailsRequest"/>
   <xs:element name="updateTransportMovementDetailsResponse" type="merc:UpdateTransportMovementDetailsResponse"/>

   <xs:element name="updateVeterinaryEventsRequest" type="merc:UpdateVeterinaryEventsRequest"/>
   <xs:element name="updateVeterinaryEventsResponse" type="merc:UpdateVeterinaryEventsResponse"/>

   <xs:element name="checkShipmentRegionalizationRequest" type="merc:CheckShipmentRegionalizationRequest"/>
   <xs:element name="checkShipmentRegionalizationResponse" type="merc:CheckShipmentRegionalizationResponse"/>

   <xs:element name="addBusinessEntityUserRequest" type="merc:AddBusinessEntityUserRequest"/>
   <xs:element name="addBusinessEntityUserResponse" type="merc:AddBusinessEntityUserResponse"/>

   <xs:element name="getBusinessEntityUserListRequest" type="merc:GetBusinessEntityUserListRequest"/>
   <xs:element name="getBusinessEntityUserListResponse" type="merc:GetBusinessEntityUserListResponse"/>

   <xs:element name="getBusinessEntityUserRequest" type="merc:GetBusinessEntityUserRequest"/>
   <xs:element name="getBusinessEntityUserResponse" type="merc:GetBusinessEntityUserResponse"/>

